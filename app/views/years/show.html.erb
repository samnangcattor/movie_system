<% meta title: t("app_year", year: @year.number), description: t("description"),
  keywords: %w(movies khmer moviehd moviehdkh watch) %>
<div class="container">
  <% if @movies.present? %>
    <div class="category">
      <h2 class="text-left" id="category-name"><%= @year.number %></h2>
      <hr/>
      <ul class="list-unstyled list-inline">
        <%= render @movies %>
      </ul>
      <%= paginate @movies %>
    </div>
  <% end %>
</div>
